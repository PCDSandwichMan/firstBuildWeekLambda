n@mixin F($j: center, $a: center, $d: row) {\n  display: flex;\n  justify-content: $j;\n  align-items: $a;\n  flex-direction: $d;\n}\n\n// TODO fix and remove above\n\n#main__ourWork {\n  margin: 35vh 0;\n}\n\n.active {\n  background: $primary;\n  a {\n    color: #fff !important;\n  }\n}\n\n#ourWork__btnFilter {\n  @include F(space-evenly);\n  margin: 0 10vw;\n  div {\n    padding: 20px 30px;\n    border-radius: 0.5rem;\n    transition: all 300ms ease-in-out;\n    &:hover {\n      cursor: pointer;\n      background: $primary;\n      a {\n        color: #fff;\n      }\n    }\n    a {\n      color: #111;\n      text-decoration: none;\n      text-transform: uppercase;\n      font-weight: 300;\n      font-size: 0.8rem;\n      letter-spacing: 0.25px;\n      transition: all 300ms ease-in-out;\n    }\n  }\n}\n\n#ourWork__example-container {\n  @include F(space-evenly);\n  flex-wrap: wrap;\n  width: 90%;\n  margin: 20px auto;\n}\n\n.ourWork__example {\n  position: relative;\n  width: 30%;\n  margin: 15px 0;\n  .plus-container {\n    opacity: 0;\n    @include F();\n    height: 3.5rem;\n    width: 3.5rem;\n    background: $primary;\n    border-radius: 50%;\n    z-index: 5;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    position: absolute;\n    transition: all 200ms ease-in-out;\n    .fa-plus {\n      color: white;\n      font-size: 2.5rem;\n    }\n    &:hover {\n      transform: translate(-50%, -50%) scale(1.1);\n    }\n  }\n  &:hover {\n    .plus-container {\n      opacity: 1;\n      cursor: pointer;\n    }\n    img:hover {\n      filter: brightness(0.3);\n    }\n  }\n  img {\n    width: 100%;\n    height: 100%;\n    transition: 350ms all ease-in-out;\n  }\n}\n\n@media only screen and (max-device-width: 1000px) {\n  #ourWork__btnFilter {\n    div {\n      a {\n        font-weight: 300;\n        font-size: 1.1rem;\n      }\n    }\n  }\n}\n\n@media only screen and (max-device-width: 860px) {\n  .ourWork__example {\n    width: 20rem;\n  }\n\n  #ourWork__btnFilter {\n    div {\n      @include F();\n      a {\n        text-align: center;\n      }\n    }\n  }\n}\n\n@media only screen and (max-device-width: 724px) {\n  #ourWork__example-container {\n    @include F(space-evenly, center, column);\n    flex-wrap: wrap;\n    width: 90%;\n    margin: 20px auto;\n  }\n\n  #ourWork__btnFilter {\n    display: none;\n  }\n\n  .ourWork__example {\n    width: 65%;\n    &:nth-child(even) {\n      display: none;\n    }\n\n    .plus-container {\n      height: 4.5rem;\n      width: 4.5rem;\n      .fa-plus {\n        color: white;\n        font-size: 3.5rem;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"fb471c3b9e80755acb4e65057be38db7","cacheData":{"env":{}}}