rgin: 30px 0;\n      h4 {\n        margin: 0 0 25px;\n        text-transform: uppercase;\n        font-size: 2.4rem;\n      }\n      #about-p {\n        font-size: 1.5rem;\n        line-height: 2.2rem;\n      }\n      a {\n        color: #999;\n        text-transform: uppercase;\n        font-size: 1.4rem;\n        transition: 100ms all ease-in;\n      }\n    }\n  }\n\n  .contact-subContainer {\n    margin: 0 0 10px;\n    i {\n      font-size: 1.4rem;\n    }\n    h5 {\n      font-size: 1.6rem;\n    }\n    h6 {\n      font-size: 1.6rem;\n    }\n  }\n\n  #footer__contact {\n    p {\n      font-size: 1.6rem;\n    }\n    form {\n      width: 100%;\n      height: 70px;\n      input {\n        font-size: 2rem;\n      }\n      button {\n        font-size: 1.6rem;\n        padding: 0 30px;\n      }\n    }\n  }\n\n  #contact__subNote {\n    font-size: 1.2rem !important;\n  }\n\n  #footer__copyright {\n    padding: 3vh 50px;\n    h6 {\n      font-size: 1.2rem;\n      font-weight: 600;\n      color: #999;\n    }\n    div {\n      margin: 0 15px 0 0;\n      i {\n        font-size: 1.5rem;\n        padding: 0 15px;\n      }\n    }\n  }\n}\n\n@media only screen and (max-device-width: 688px) {\n  #footer__info-container {\n    section {\n      h4 {\n        font-size: 2rem;\n      }\n      #about-p {\n        font-size: 1.4rem;\n        line-height: 2rem;\n      }\n      a {\n        font-size: 1.3rem;\n      }\n    }\n  }\n\n  .contact-subContainer {\n    margin: 0 0 10px;\n    i {\n      font-size: 1.4rem;\n    }\n    h5 {\n      font-size: 1.6rem;\n    }\n    h6 {\n      font-size: 1.6rem;\n    }\n  }\n\n  #footer__contact {\n    p {\n      font-size: 1.5rem;\n    }\n    form {\n      height: 60px;\n      input {\n        font-size: 1.8rem;\n      }\n      button {\n        font-size: 1.3rem;\n      }\n    }\n  }\n\n  #contact__subNote {\n    font-size: 1.3rem !important;\n  }\n\n  #footer__copyright {\n    @include F(space-evenly, center, column);\n    padding: 3vh 50px;\n    