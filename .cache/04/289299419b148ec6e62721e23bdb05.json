ound: #222;\n    padding: 40px 0;\n    h2 {\n      text-align: center;\n      font-size: 1.7rem;\n      color: white;\n    }\n  }\n  hr {\n    border: none;\n    width: 100%;\n    height: 1px;\n    background: #dadada;\n  }\n}\n\n.card_stats-container {\n  @include F(center, center, column);\n  margin: 30px 0;\n  h3 {\n    margin: 15px 0;\n    font-size: 1rem;\n    font-weight: 400;\n    color: #999;\n    strong {\n      color: #222;\n    }\n  }\n}\n\n.card_pricing-container {\n  @include F(center, center, column);\n  margin: 0 auto;\n  h1 {\n    color: #333;\n    position: relative;\n    font-size: 4rem;\n    font-weight: 400;\n    margin: 20px 0 0;\n    &::before {\n      content: '$';\n      color: #333;\n      position: absolute;\n      font-size: 1.5rem;\n      font-weight: 400;\n      transform: translate(-120%, 25%);\n    }\n  }\n  h4 {\n    color: #afafaf;\n    font-weight: 200;\n  }\n}\n\n.card__btn {\n  padding: 15px 30px;\n  background: $primary;\n  border-radius: 0.25rem;\n  margin: 40px 0;\n  transition: all 100ms ease-in-out;\n  &:hover {\n    cursor: pointer;\n    background: #4dcc9b;\n  }\n  a {\n    color: #fff;\n    text-decoration: none;\n  }\n}\n\n@media only screen and (max-device-width: 1344px) {\n  .pricing-card {\n    width: 350px;\n    .card__title {\n      h2 {\n        text-align: center;\n        font-size: 2.5rem;\n      }\n    }\n  }\n\n  .card_stats-container {\n    h3 {\n      font-size: 1.5rem;\n      font-weight: 400;\n      color: #999;\n    }\n  }\n\n  .card_pricing-container {\n    h1 {\n      font-size: 5rem;\n    }\n    h4 {\n      font-size: 1.5rem;\n    }\n  }\n\n  .card__btn {\n    a {\n      font-size: 1.5rem;\n    }\n  }\n}\n\n@media only screen and (max-device-width: 1188px) {\n  #cards-container {\n    @include F(center, center, column);\n  }\n\n  .pricing-card {\n    width: 600px;\n    margin: 50px 0px;\n    box-shadow: 2px 2px 6px #999;\n    .card__title {\n      h2 {\n        text-align: center;\n        font-size: 3rem;\n      }\n    }\n  }\n\n  .card_stats-container {\n    h3 {\n      margin: 25px 0;\n      font-size: 2rem;\n    }\n  }\n\n  .card_pricing-container {\n    h1 {\n      font-size: 6rem;\n    }\n    h4 {\n      font-size: 1.7rem;\n    }\n  }\n\n  .card__btn {\n    a {\n      font-size: 2rem;\n    }\n  }\n}\n\n@media only screen and (max-device-width: 668px) {\n  #cards-container {\n    @include F(center, center, column);\n  }\n\n  .pricing-card {\n    width: 400px;\n    margin: 20px 0px;\n    .card__title {\n      h2 {\n        text-align: center;\n        font-size: 3rem;\n      }\n    }\n  }\n\n  .card_stats-container {\n    h3 {\n      margin: 15px 0;\n      font-size: 1.8rem;\n    }\n  }\n\n  .card_pricing-container {\n    h1 {\n      font-size: 4.5rem;\n    }\n    h4 {\n      font-size: 1.2rem;\n    }\n  }\n\n  .card__btn {\n    a {\n      font-size: 1.8rem;\n    }\n  }\n}"},"lineCount":null}},"error":null,"hash":"e9aadba47993ebe1fe3fa0341769de38","cacheData":{"env":{}}}