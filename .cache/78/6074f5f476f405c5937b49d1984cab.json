  width: 100%;\n  height: 100%;\n  background: rgba(17, 17, 17, 0.719);\n}\n\n#hero__text {\n  @include F(center, flex-start, column);\n}\n\n#header__hero {\n  position: absolute;\n  top: 45%;\n  left: 10%;\n  transform: translate(0, -50%);\n\n  width: 50vw;\n  h1 {\n    color: $primary;\n    font-size: 3rem;\n  }\n  h2 {\n    color: #fff;\n    font-size: 3rem;\n    margin: 20px 0;\n  }\n  p {\n    color: #fff;\n    font-size: 1.2rem;\n    margin: 0 0 20px;\n  }\n}\n\n#hero__callToAction {\n  align-self: flex-start;\n  background: $primary;\n  border-radius: 0.3rem;\n  // padding: 20px 35px;\n  height: 50px;\n  @include F();\n  transition: 200ms all ease-in-out;\n  &:hover {\n    cursor: pointer;\n    background: #3b9e78;\n  }\n  a {\n    text-decoration: none;\n    color: white;\n    padding: 40px;\n  }\n}\n\n@media only screen and (max-device-width: 1000px) {\n  #hero__text {\n    @include F(center, center, column);\n  }\n\n  #header__hero {\n    width: 90%;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    h1 {\n      font-size: 4rem;\n    }\n    h2 {\n      font-size: 4rem;\n    }\n    p {\n      font-size: 1.8rem;\n      text-align: center;\n    }\n  }\n\n  #hero__callToAction {\n    margin: 20px 0;\n    align-self: center;\n    padding: 25px 45px;\n    transition: 200ms all ease-in-out;\n    a {\n      font-size: 1.5rem;\n    }\n  }\n}\n\n@media only screen and (max-device-width: 1000px) {\n  #header__hero {\n    h1 {\n      font-size: 3.2rem;\n      text-align: center;\n    }\n    h2 {\n      text-align: center;\n      font-size: 3rem;\n    }\n    p {\n      font-size: 1.6rem;\n      text-align: center;\n    }\n  }\n\n  #hero__callToAction {\n    margin: 20px 0;\n    align-self: center;\n    padding: 25px 45px;\n    transition: 200ms all ease-in-out;\n    a {\n      font-size: 1.5rem;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"882c802e32cfa768f75a53a1a98de177","cacheData":{"env":{}}}