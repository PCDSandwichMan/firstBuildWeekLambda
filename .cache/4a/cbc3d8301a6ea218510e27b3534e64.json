tart;\n  background: $primary;\n  border-radius: 0.3rem;\n  padding: 20px 35px;\n  transition: 200ms all ease-in-out;\n  &:hover {\n    cursor: pointer;\n    background: #3b9e78;\n  }\n  a {\n    text-decoration: none;\n    color: white;\n  }\n}\n\n@media only screen and (max-device-width: 1000px) {\n  #hero__text {\n    @include F(center, center, column);\n  }\n\n  #header__hero {\n    width: 90%;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    h1 {\n      font-size: 4rem;\n    }\n    h2 {\n      font-size: 4rem;\n    }\n    p {\n      font-size: 1.8rem;\n      text-align: center;\n    }\n  }\n\n  #hero__callToAction {\n    margin: 20px 0;\n    align-self: center;\n    padding: 25px 45px;\n    transition: 200ms all ease-in-out;\n    a {\n      font-size: 1.5rem;\n    }\n  }\n}\n\n\n@media only screen and (max-device-width: 1000px) {\n\n  #header__hero {\n    h1 {\n      font-size: 3.2rem;\n      text-align: center;\n    }\n    h2 {\n      text-align: center;\n      font-size: 3rem;\n    }\n    p {\n      font-size: 1.6rem;\n      text-align: center;\n    }\n  }\n\n  #hero__callToAction {\n    margin: 20px 0;\n    align-self: center;\n    padding: 25px 45px;\n    transition: 200ms all ease-in-out;\n    a {\n      font-size: 1.5rem;\n    }\n  }\n}"},"lineCount":null}},"error":null,"hash":"5a941752e9db44f1ffa77d6a154170dc","cacheData":{"env":{}}}