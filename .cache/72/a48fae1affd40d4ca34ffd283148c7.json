ont-weight: 400;\n    color: #999;\n    strong {\n      color: #222;\n    }\n  }\n}\n\n.card_pricing-container {\n  @include F(center, center, column);\n  margin: 0 auto;\n  h1 {\n    color: #333;\n    position: relative;\n    font-size: 4rem;\n    font-weight: 400;\n    margin: 20px 0 0;\n    &::before {\n      content: '$';\n      color: #333;\n      position: absolute;\n      font-size: 1.5rem;\n      font-weight: 400;\n      transform: translate(-120%, 25%);\n    }\n  }\n  h4 {\n    color: #afafaf;\n    font-weight: 200;\n  }\n}\n\n.card__btn {\n  padding: 15px 30px;\n  background: $primary;\n  border-radius: 0.25rem;\n  margin: 40px 0;\n  transition: all 100ms ease-in-out;\n  &:hover {\n    cursor: pointer;\n    background: #4dcc9b;\n  }\n  a {\n    color: #fff;\n    text-decoration: none;\n  }\n}\n\n@media only screen and (max-device-width: 1344px) {\n  .pricing-card {\n    width: 350px;\n    .card__title {\n      h2 {\n        text-align: center;\n        font-size: 2.5rem;\n      }\n    }\n  }\n\n  .card_stats-container {\n    h3 {\n      font-size: 1.5rem;\n      font-weight: 400;\n      color: #999;\n    }\n  }\n\n  .card_pricing-container {\n    h1 {\n      font-size: 5rem;\n    }\n    h4 {\n      font-size: 1.5rem;\n    }\n  }\n\n  .card__btn {\n    a {\n      font-size: 1.5rem;\n    }\n  }\n}\n\n@media only screen and (max-device-width: 1188px) {\n  #cards-container {\n    @include F(center, center, column);\n  }\n\n  .pricing-card {\n    width: 600px;\n    margin: 30px 0px;\n    box-shadow: 2px 2px 6px #999;\n    .card__title {\n      h2 {\n        text-align: center;\n        font-size: 2rem;\n      }\n    }\n  }\n\n  .card_stats-container {\n    h3 {\n      margin: 15px 0;\n      font-size: 1.5rem;\n    }\n  }\n\n  .card_pricing-container {\n    h1 {\n      font-size: 4rem;\n    }\n    h4 {\n      font-size: 1rem;\n    }\n  }\n\n  .card__btn {\n    a {\n      font-size: 1.5rem;\n    }\n  }\n}\n\n@media only screen and (max-device-width: 869px) {\n  .pricing-card {\n    width: 450px;\n    margin: 15px 0px;\n    .card__title {\n      padding: 20px;\n      h2 {\n        font-size: 2rem;\n      }\n    }\n  }\n}\n\n@media only screen and (max-device-width: 707px) {\n  .pricing-card {\n    width: 400px;\n    margin: 15px 0px;\n    .card__title {\n      padding: 15px;\n      h2 {\n        font-size: 2rem;\n      }\n    }\n  }\n\n  .card_stats-container {\n    h3 {\n      margin: 10px 0;\n      font-size: 1.3rem;\n    }\n  }\n\n  .card_pricing-container {\n    h1 {\n      font-size: 4rem;\n    }\n    h4 {\n      font-size: 1rem;\n    }\n  }\n\n  .card__btn {\n    margin: 20px 0;\n    a {\n      font-size: 1.5rem;\n    }\n  }\n}"},"lineCount":null}},"error":null,"hash":"14ff0e537eb20b527445c711b71b2da7","cacheData":{"env":{}}}