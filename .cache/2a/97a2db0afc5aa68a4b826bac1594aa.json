es/1-Navs/nav.scss","name":null,"original":{"line":142,"column":17},"generated":{"line":156,"column":15}},{"source":"styles/1-Navs/nav.scss","name":null,"original":{"line":143,"column":10},"generated":{"line":157,"column":4}},{"source":"styles/1-Navs/nav.scss","name":null,"original":{"line":143,"column":26},"generated":{"line":157,"column":20}},{"source":"styles/1-Navs/nav.scss","name":null,"original":{"line":144,"column":10},"generated":{"line":158,"column":4}},{"source":"styles/1-Navs/nav.scss","name":null,"original":{"line":144,"column":19},"generated":{"line":158,"column":15}},{"source":"styles/1-Navs/nav.scss","name":null,"original":{"line":145,"column":9},"generated":{"line":159,"column":4}},{"source":"styles/1-Navs/nav.scss","name":null,"original":{"line":145,"column":9},"generated":{"line":159,"column":22}}],"sources":{"styles/1-Navs/nav.scss":"@import url('../0-Globals/mixins.scss');\n@import url('../0-Globals/variables.scss');\n\n@mixin F($j: center, $a: center, $d: row) {\n  display: flex;\n  justify-content: $j;\n  align-items: $a;\n  flex-direction: $d;\n}\n\n$primary: #42b186;\n$secondary: #b1426d;\n$tertiary: #d999b2;\n// TODO fix mixin imports\n\n#main-nav {\n  position: sticky;\n  top: 0;\n  z-index: 10;\n  background: #f5f5f5;\n  box-shadow: 1px 1px 4px #c5c5c5;\n  @include F(space-between);\n  padding: 0 10%;\n  height: 90px;\n  div {\n    width: 60px;\n    height: 100%;\n    img {\n      width: 100%;\n      height: 100%;\n      &:hover {\n        cursor: pointer;\n      }\n    }\n  }\n  ul {\n    @include F(space-evenly);\n    height: 100%;\n    li {\n      position: relative;\n      @include F();\n      margin: 0 1px;\n      height: 100%;\n      transition: all 250ms ease-in-out;\n      &::before {\n        content: '';\n        opacity: 0;\n        position: absolute;\n        top: 0;\n        transform: translateY(-100%);\n        height: 5px;\n        width: 100%;\n        background: $primary;\n        transition: all 250ms ease-in-out;\n      }\n      &:hover {\n        cursor: pointer;\n        background: #ebebeb;\n        &::before {\n          opacity: 1;\n        }\n        a {\n          color: $primary;\n        }\n      }\n      a {\n        text-decoration: none;\n        font-size: 1rem;\n        color: #222;\n        padding: 0 20px;\n        transition: all 200ms ease-in-out;\n      }\n    }\n  }\n}\n\n// Responsive classes\n.show {\n  height: 450px !important;\n}\n\n@media only screen and (max-device-width: 1174px) {\n  #main-nav {\n    padding: 0 1%;\n  }\n}\n\n@media only screen and (max-device-width: 918px) {\n  .fa-bars {\n    position: absolute;\n    right: 5%;\n    display: block !important;\n    font-size: 1.2rem;\n    background: $primary;\n    padding: 15px;\n    color: #fff;\n    border-radius: 0.25rem;\n    &:hover {\n      cursor: pointer;\n    }\n  }\n\n  #main-nav {\n    z-index: 10;\n    @include F(space-between);\n    ul {\n      z-index: 11;\n      height: 0px;\n      overflow: hidden;\n      width: 80vw;\n      position: absolute;\n      top: 103%;\n      left: 50%;\n      transform: translate(-50%, 0);\n      background: #222;\n      @include F(space-evenly, center, column);\n      transition: all 500ms ease-in-out;\n      li {\n        @include F(flex-start, center);\n        height: 100%;\n        width: 100%;\n        padding: 20px 0;\n        margin: 0px;\n        &:not(:last-child) {\n          border-bottom: 1px solid #888;\n        }\n        &::before {\n          display: none;\n        }\n        &:hover {\n          cursor: pointer;\n          background: #2c2c2c;\n          &::before {\n            display: none;\n          }\n          a {\n            color: $primary;\n          }\n        }\n        a {\n          font-size: 1.3rem;\n          color: #c2c2c2;\n          letter-spacing: 1.2px;\n          padding: 0 20px;\n        }\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"36e49567b8e1f24909ac32a87e67a02f","cacheData":{"env":{}}}