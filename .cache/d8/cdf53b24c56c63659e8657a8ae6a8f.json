\n    background: $primary;\n    padding: 20px 25px;\n    color: #fff;\n    font-size: 0.8rem;\n    font-weight: 400;\n    border: none;\n  }\n}\n\n#btnForm-wrapper {\n  width: 100%;\n  @include F();\n}\n\n#contactForm__container {\n  height: 25vh;\n  margin: 0 10vw;\n  form {\n    @include F(space-evenly);\n    width: 100%;\n    height: 100%;\n    div {\n      @include F(space-between, center, $d: column);\n      height: 100%;\n      input {\n        font-size: 0.8rem;\n        letter-spacing: 1.5px;\n        color: #665;\n        width: 35vw;\n        padding: 15px;\n        background: #eee;\n        border: 0.5px solid #ddd;\n        outline: none;\n        &:focus {\n          outline: 1px solid #4ed19f;\n        }\n      }\n    }\n    textarea {\n      height: 100%;\n      padding: 15px;\n      color: #665;\n      font-size: 0.8rem;\n      letter-spacing: 1.5px;\n      width: 50%;\n      background: #eee;\n      border: 0.5px solid #ddd;\n      &:focus {\n        outline: 1px solid #4ed19f;\n      }\n    }\n  }\n}\n\n@media only screen and (max-device-width: 1400px) {\n  #contactForm__container {\n    form {\n      div {\n        @include F(space-evenly, center, column);\n        input {\n          font-size: 1.2rem;\n          padding: 25px;\n          &:focus {\n            outline: 1px solid #4ed19f;\n          }\n        }\n      }\n      textarea {\n        font-size: 1.2rem;\n      }\n    }\n  }\n}\n\n@media only screen and (max-device-width: 988px) {\n  #contactForm__container {\n    height: 45vh;\n    form {\n      @include F(space-evenly, center, column);\n      margin: 0 auto;\n      width: 85%;\n      div {\n        @include F(space-evenly, center, column);\n        width: 100%;\n        input {\n          font-size: 1.3rem;\n          width: 100%;\n          margin: 5px 0;\n          padding: 15px;\n        }\n      }\n      textarea {\n        margin: 5px 0;\n        font-size: 1.3rem;\n        width: 100%;\n      }\n    }\n  }\n\n  #main__contact {\n    button {\n      margin: 2vh auto;\n      font-size: 1.8rem;\n    }\n  }\n}\n\n@media only screen and (max-device-width: 644px) {\n  #contactForm__container {\n    form {\n      width: 100%;\n      div {\n        @include F(space-evenly, center, column);\n        width: 100%;\n        input {\n          font-size: 1.5rem;\n          margin: 3px 0;\n        }\n      }\n      textarea {\n        font-size: 1.5rem;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"fe3eac501bb0c333dc2b2c2d7080b7d8","cacheData":{"env":{}}}